<h1>Enter a new contact:</h1>

<%= form_tag '/contacts', method: :post do %>
<p><%= label_tag :first_name %>
<%= text_field_tag :first_name %></p>
<p><%= label_tag :middle_name %>
<%= text_field_tag :middle_name %></p>
<p><%= label_tag :last_name %>
<%= text_field_tag :last_name %></p>
<p><%= label_tag :email %>
<%= text_field_tag :email %></p>
<p><%= label_tag :phone_number %>
<%= text_field_tag :phone_number %></p>
<p><%= label_tag :address %>
<%= text_field_tag :address %></p>
<p><%= label_tag :bio %>
<%= text_area_tag :bio %></p>
<p><%= submit_tag "Submit" %></p>
<% end %>